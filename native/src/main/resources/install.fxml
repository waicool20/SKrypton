<GridPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="300.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.162-ea" xmlns:fx="http://javafx.com/fxml/1">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints prefHeight="75.0" vgrow="ALWAYS" />
  </rowConstraints>
   <children>
      <VBox fx:id="descriptionBox" alignment="CENTER" prefWidth="400.0">
         <children>
            <Text fx:id="progressText" strokeType="OUTSIDE" strokeWidth="0.0" text="File to load" />
            <ProgressBar fx:id="progressBar" prefWidth="350.0" progress="0.0" />
            <HBox>
               <children>
                  <TextArea fx:id="progressTextArea" prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </TextArea>
               </children>
            </HBox>
            <Button fx:id="closeButton" mnemonicParsing="false" text="Close" visible="false" />
         </children></VBox>
   </children>
</GridPane>
